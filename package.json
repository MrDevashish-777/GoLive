{
  "name": "livekit-backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "scripts": {
    "dev:api": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:web": "expo start --clear",
    "dev": "concurrently \"npm:dev:api\" \"npm:dev:web\"",
    "build": "tsc",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^7.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "envalid": "^8.1.0",
    "expo-haptics": "^14.1.4",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.0.0",
    "livekit-server-sdk": "^2.13.3",
    "morgan": "^1.10.0",
    "tslib": "^2.6.2",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "concurrently": "^9.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  },
  "private": true
}
